{
  "name": "@fullstack-example/api",
  "scripts": {
    "db:schema": "untype pg schema src/migrations untype",
    "db:reset": "untype pg reset -n untype -p 5434",
    "db:migration:create": "untype migrations create src/migrations",
    "db:migration:regenerate": "untype migrations regenerate src/migrations",
    "db:migration:apply": "ts-node src/migrations/MigrationRunner.ts -p tsconfig.migrations.json",
    "db:entities:generate": "untype orm generate src/entities -n untype -p 5434",
    "db:dev": "npm-run-all -l -n 'db:migration:apply' 'db:schema' 'db:entities:generate'",
    "db:dev:reset": "npm-run-all -l -n 'db:reset' 'db:migration:apply' 'db:schema' 'db:entities:generate'"
  },
  "exports": {
    ".": {
      "require": "./dist/index.js"
    }
  },
  "dependencies": {
    "@untype/cli": "workspace:*",
    "@untype/config": "workspace:*",
    "@untype/core": "workspace:*",
    "@untype/geo": "workspace:*",
    "@untype/logger": "workspace:*",
    "@untype/orm": "workspace:*",
    "@untype/rpc": "workspace:*",
    "@untype/rpc-express": "workspace:*",
    "@untype/worker": "workspace:*",
    "google-auth-library": "8.8.0",
    "reflect-metadata": "0.1.13",
    "source-map-support": "0.5.21",
    "tsyringe": "4.7.0",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@jest/globals": "29.5.0",
    "npm-run-all": "4.1.5",
    "testcontainers": "9.8.0",
    "type-fest": "3.10.0"
  }
}
